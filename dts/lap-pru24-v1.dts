/* Based on https://github.com/beagleboard/bb.org-overlays/blob/master/src/arm/BB-BBBMINI-00A0.dts */
/* With information from http://elinux.org/Ti_AM33XX_PRUSSv2#Beaglebone_PRU_connections_and_modes */

/dts-v1/;
/plugin/;

/ {
	compatible = "ti,beaglebone", "ti,beaglebone-black", "ti,beaglebone-green";

	/* identification */
	part-number = "BB-LAP-PRU24-V1";
	version = "00A0";

	/* state the resources this cape uses */
	exclusive-use =
		"P9.31",
		"P9.29",
		"P9.30",
		"P9.28",
		"P9.42",
		"P9.27",
		"P9.41",
		"P9.25",
		"P8.12",
		"P8.11",
		"P8.45",
		"P8.46",
		"P8.43",
		"P8.44",
		"P8.41",
		"P8.42",
		"P8.39",
		"P8.40",
		"P8.27",
		"P8.29",
		"P8.28"
	;

	fragment@0 {
		target = <&am33xx_pinmux>;
		__overlay__ {
			am33xx_pinmux: data {
				pinctrl-single,pins = <
					0x190 0xd
					0x194 0xd
					0x198 0xd
					0x19C 0xd
					0x1A0 0xd
					0x1A4 0xd
					0x1A8 0xd
					0x1AC 0xd
					0x030 0xe
					0x034 0xe
					0x0A0 0xd
					0x0A4 0xd
					0x0A8 0xd
					0x0AC 0xd
					0x0B0 0xd
					0x0B4 0xd
					0x0B8 0xd
					0x0BC 0xd
					0x0E0 0xd
					0x0E4 0xd
					0x0E8 0xd
					0x0EC 0xd
				>;
			};
		};
	};

	fragment@1 {
			target = <&ocp>;
			__overlay__ {
				bs_pinmode_P8_30_0xd_pinmux {
					compatible = "bone-pinmux-helper";
					status = "okay";
					pinctrl-names = "default";
					pinctrl-0 = <&am33xx_pinmux>;
				};
			};
		};
};

